<!-- Desktop Navigation -->
<nav class="grid-x grid-padding-x grid-padding-y align-justify align-middle" id="swup-header">

  <div class="small-4 cell align-middle">
    <button class="hamburger hamburger--criss-cross" id="menuBar" type="button" data-toggle="menuBar offCanvasLeft" data-toggler="active">
        <div class="inner">
            <span class="bar"></span>
            <span class="bar"></span>
            <span class="bar"></span>
        </div>
    </button>
  </div>

  <div class="small-4 cell text-center">
    <div style="overflow: hidden">
      <div class="cl-effect-5" id="cl-effect-5">
          <a href="/" class="logo text-uppercase letter-spacing--loose h5"><span data-hover="{{ currentSite.name }}">{{ currentSite.name }}</span></a>
      </div>
    </div>
  </div>

  <div class="small-4 cell align-middle text-right">
    <a href="/contact"><svg class="icon icon-search"><use xlink:href="/assets/icons/symbol-defs.svg#icon-search"></use></svg></a>
  </div>
